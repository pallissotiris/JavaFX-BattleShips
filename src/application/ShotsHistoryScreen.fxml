<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="280.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ShotsHistoryController">
   <children>
      <VBox alignment="CENTER" maxHeight="280.0" spacing="20.0">
         <children>
            <Label fx:id="titleLabel" text="Example" textAlignment="CENTER">
               <font>
                  <Font name="Stencil" size="27.0" />
               </font></Label>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label text="Coordinates">
                           <font>
                              <Font name="Stencil" size="20.0" />
                           </font></Label>
                        <ListView fx:id="coordinatesList" prefWidth="60.0" style="-fx-font-size: 15.0;">
                           <padding>
                              <Insets bottom="20.0" />
                           </padding></ListView>
                     </children>
                     <padding>
                        <Insets bottom="20.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
                     <children>
                        <Label text="Result">
                           <font>
                              <Font name="Stencil" size="20.0" />
                           </font></Label>
                        <ListView fx:id="resultList" prefWidth="60.0" style="-fx-font-size: 15.0;">
                           <padding>
                              <Insets bottom="20.0" />
                           </padding></ListView>
                     </children>
                     <padding>
                        <Insets bottom="20.0" />
                     </padding>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
                     <children>
                        <Label text="Ship">
                           <font>
                              <Font name="Stencil" size="20.0" />
                           </font></Label>
                        <ListView fx:id="shipList" prefWidth="60.0" style="-fx-font-size: 15.0;">
                           <padding>
                              <Insets bottom="20.0" />
                           </padding></ListView>
                     </children>
                     <padding>
                        <Insets bottom="20.0" />
                     </padding>
                  </VBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
